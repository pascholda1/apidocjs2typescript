{
	"name": "apidocjs2typescript",
	"description": "Generate a ready-to-use API connection based on its [ApiDocJs](http://apidocjs.com) documentation.",
	"keywords": [
		"apidoc",
		"apidocjs",
		"typescript",
		"code generator"
	],
	"author": "Simon Paschold (https://github.com/pascholda1)",
	"version": "1.1.3",
	"license": "MIT",
	"main": "dist/index.js",
	"bin": {
		"apidocjs2typescript": "dist/index.js"
	},
	"scripts": {
		"build": "./node_modules/.bin/tsc",
		"prepare": "npm run build",
		"run": "node ./dist/index.js --docs-json=./dummyData/api-data.json --api-name=v-1-2-0 --out=./dummyData/api"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pascholda1/apidocjs2typescript.git"
	},
	"files": [
		"dist",
		"static"
	],
	"dependencies": {
		"fs": "^0.0.1-security",
		"lodash": "^4.17.21",
		"minimist": "^1.2.8",
		"node": "^22.13.1",
		"path": "^0.12.7",
		"qs": "^6.14.0"
	},
	"devDependencies": {
		"@types/lodash.set": "4.3.9",
		"@types/minimist": "^1.2.5",
		"@types/node": "^22.13.10",
		"@types/qs": "6.9.18",
		"typescript": "5.6.3"
	}
}
